<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.zxkj.assitance"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="17"
        android:targetSdkVersion="21" />

    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.KILL_BACKGROUND_PROCESSES" />
    <uses-permission android:name="android.permission.CAMERA" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launchabc"
        android:label="@string/app_name"
        android:theme="@android:style/Theme.Holo.Light.NoActionBar.Fullscreen" >
        <activity
            android:name=".main.MainActivity"
            android:label="@string/app_name" >
        </activity>
        <activity android:name=".main.LeadActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".main.LogoActivity"
            android:label="@string/title_activity_logo" >
        </activity>
        <activity
            android:name=".DBActivity"
            android:label="@string/title_activity_db" >
        </activity>
        <activity
            android:name=".main.TelMgrActivity"
            android:label="@string/title_activity_tel_mgr" >
        </activity>
        <activity
            android:name=".main.FileMgrActivity"
            android:label="@string/title_activity_file_mgr"
            android:parentActivityName="android.accounts.AccountAuthenticatorActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="android.accounts.AccountAuthenticatorActivity" />
        </activity>
        <activity
            android:name=".main.FileDetailActivity"
            android:label="@string/title_activity_file_detail" >
        </activity>
        <activity
            android:name=".CleanActivity"
            android:label="@string/title_activity_clean" >
        </activity>
        <activity
            android:name=".SoftManagerActivity"
            android:label="@string/title_activity_soft_manager" >
        </activity>
        <activity
            android:name=".SftAppActivity"
            android:label="@string/title_activity_sft_app" >
        </activity>
        <activity
            android:name=".SpeedActivity"
            android:label="@string/title_activity_speed" >
        </activity>
        <activity
            android:name=".DecetionActivity"
            android:label="@string/title_activity_decetion" >
        </activity>
        <activity
            android:name=".main.TelInfoActivity"
            android:label="@string/title_activity_tel_info" >
        </activity>
        <activity
            android:name=".SettingActivity"
            android:label="@string/title_activity_setting" >
        </activity>
    </application>

</manifest>
